gyCheckboxes.js","original":{"line":42,"column":8}},{"generated":{"line":47,"column":9},"source":"src/GUI/strategyCheckboxes.js","original":{"line":42,"column":9},"name":"addEventListener"},{"generated":{"line":47,"column":25},"source":"src/GUI/strategyCheckboxes.js","original":{"line":42,"column":0}},{"generated":{"line":47,"column":26},"source":"src/GUI/strategyCheckboxes.js","original":{"line":42,"column":26}},{"generated":{"line":47,"column":44},"source":"src/GUI/strategyCheckboxes.js","original":{"line":42,"column":0}},{"generated":{"line":47,"column":46},"source":"src/GUI/strategyCheckboxes.js","original":{"line":42,"column":46}},{"generated":{"line":47,"column":58},"source":"src/GUI/strategyCheckboxes.js","original":{"line":42,"column":58}},{"generated":{"line":48,"column":2},"source":"src/GUI/strategyCheckboxes.js","original":{"line":43,"column":2},"name":"renderCheckboxes"},{"generated":{"line":48,"column":18},"source":"src/GUI/strategyCheckboxes.js","original":{"line":43,"column":18}},{"generated":{"line":49,"column":0},"source":"src/GUI/strategyCheckboxes.js","original":{"line":44,"column":1}},{"generated":{"line":49,"column":1},"source":"src/GUI/strategyCheckboxes.js","original":{"line":42,"column":0}}],"sources":{"src/GUI/strategyCheckboxes.js":"import { STRATEGIES, selectedStrategies, reset } from \"../index\";\n\nfunction strategyName(strategy) {\n  return strategy\n    .toLowerCase()\n    .replace(/^_*(.)|_+(.)/g, (s, c, d) =>\n      c ? c.toUpperCase() : \" \" + d.toUpperCase()\n    );\n}\n\n// Create HTML checkboxes for each strategy, and add them and remove them to selectedStrategies when enabled/disabled\nfunction renderCheckboxes() {\n  const strategyCheckboxes = document.getElementById(\"strategy-checkboxes\");\n\n  STRATEGIES.forEach((strategy) => {\n    const container = document.createElement(\"div\");\n    container.classList.add(\"form-check\");\n    const checkbox = document.createElement(\"input\");\n    checkbox.classList.add(\"form-check-input\");\n    checkbox.type = \"checkbox\";\n    checkbox.id = strategy;\n    // Check the box if it's in selectedStrategies\n    checkbox.checked = selectedStrategies.includes(strategy);\n    checkbox.addEventListener(\"change\", () => {\n      if (checkbox.checked) {\n        selectedStrategies.push(strategy);\n      } else {\n        selectedStrategies = selectedStrategies.filter((s) => s !== strategy);\n      }\n      reset();\n    });\n    const label = document.createElement(\"label\");\n    label.htmlFor = strategy;\n    label.classList.add(\"form-check-label\");\n    label.appendChild(document.createTextNode(strategyName(strategy)));\n    strategyCheckboxes.appendChild(container);\n    container.appendChild(checkbox);\n    container.appendChild(label);\n  });\n}\n\ndocument.addEventListener(\"DOMContentLoaded\", function () {\n  renderCheckboxes();\n});\n"},"lineCount":null}},"error":null,"hash":"c1ff3272a7fbbdcd5cb72947303d9b4d","cacheData":{"env":{}}}